{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Database": "Host=localhost;Port=5432;Database=identity;Username=postgres;Password=postgres;Include Error Detail=true",
    "DefaultConnectionNew": "",
    "Cache": "caheName.redis:6379",
    //"BlobStorage": "UseDevelopmentStorage=true; DevelopmentStorageProxyUri=http://blobStorageName"
    "BlobStorage": "UseDevelopmentStorage=true; DevelopmentStorageProxyUri=http://i3lab.blob-storage:10000"
  },

  "JwtOptions": {
    "SecretKey": "SecretKeySecretKeySecretKeySecretKeySecretKeySecretKeySecretKeySecretKey",
    "ExpiresTime": "12"
  },



  "MessageBroker": {
    "Port": 5672,
    //"HostName": "rabbitmq",
    "HostName": "amqp://i3lab-rabbitmq:5672",
    "UserName": "guest",
    "Password": "guest"
    //"VirtualHost": "/",
    //"ExchangeName": "default_exchange",
    //"QueueName": "default_queue",
    //"RoutingKey": "default_routing_key"
  },

  "Email": {
    "SenderEmail": "k@gmail.com",
    "Sender": "Start-Up",
    "Host": "papercut",
    "Port": 25,
 
  },


  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [

      { "Name": "Console" },

      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://i3lab-seq:5341" }
      }

      //{
      //  "Name": "File",
      //  "Args": {
      //    "path": "logs/myCrazyConfigLog-.txt",
      //    "rollingInterval": "Day"
      //  }
      //}
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }

}
